{
  "name": "rails-webpack",
  "version": "1.0.0",
  "description": "rails-webpack integration example",
  "main": "index.js",
  "scripts": {
    "dev": "webpack-dev-server --config frontend/config/webpack.dev.config",
    "prod": "webpack -p --config frontend/config/webpack.prod.config",
    "test": "jest",
    "postinstall": "npm run prod"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "webpack-dev-server": "^1.14.0"
  },
  "engines": {
    "node": "5.3.0",
    "npm": "3.3.12"
  },
  "dependencies": {
    "babel-core": "^6.3.26",
    "babel-jest": "^6.0.1",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-react-hmre": "^1.0.1",
    "classnames": "^2.2.3",
    "css-loader": "^0.23.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "jest-cli": "^0.8.2",
    "lodash": "^3.10.1",
    "node-sass": "^3.4.2",
    "react": "^0.14.5",
    "react-addons-test-utils": "^0.14.5",
    "react-dom": "^0.14.5",
    "react-hot-loader": "^1.3.0",
    "sass-loader": "^3.1.2",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.9"
  },
  "babel": {
    "presets": [
      "react",
      "es2015"
    ]
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/frontend/config/jest-preprocessor.js",
    "testPathDirs": [
      "<rootDir>/frontend/__tests__"
    ],
    "testFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json"
    ],
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/react-dom",
      "<rootDir>/node_modules/react-addons-test-utils",
      "<rootDir>/node_modules/fbjs"
    ]
  }
}
